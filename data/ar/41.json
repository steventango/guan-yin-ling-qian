{"\u7c64\u8a69\u7248\u672c\u4e8c": "\u0633\u0648\u0641 \u062a\u062a\u0630\u0643\u0631 \u0627\u0644\u0643\u0644\u0645\u0627\u062a \u0627\u0644\u0637\u064a\u0628\u0629 \u0627\u0644\u062a\u064a \u0644\u0627 \u0646\u0647\u0627\u064a\u0629 \u0644\u0647\u0627\u060c \u0648\u0644\u0643\u0646\u0643 \u0633\u062a\u0643\u0648\u0646 \u0627\u0628\u0646 \u0644\u0635.\n\u0644\u0627 \u062a\u0643\u0646 \u062c\u0634\u0639\u064b\u0627 \u0641\u064a \u0627\u0644\u0648\u0642\u062a \u0627\u0644\u062d\u0627\u0644\u064a \u0648\u062a\u0642\u0644\u0642 \u0628\u0634\u0623\u0646 \u0627\u0644\u0637\u0631\u064a\u0642\u0629 \u0627\u0644\u062a\u064a \u0643\u0627\u0646 \u0633\u064a\u0641\u0639\u0644 \u0628\u0647\u0627 \u0634\u064a\u0626\u064b\u0627 \u062c\u064a\u062f\u064b\u0627 \u0642\u0628\u0644 \u0639\u0627\u0645.", "\u8a69\u610f": "\u0647\u0630\u0627 \u0627\u0644\u0631\u0633\u0645 \u0627\u0644\u0633\u062f\u0627\u0633\u064a \u064a\u062d\u062f\u062f \u0627\u0644\u0644\u0635 \u0639\u0644\u0649 \u0623\u0646\u0647 \u0627\u0644\u0627\u0628\u0646.\n\u0643\u0646 \u062d\u0630\u0631\u0627 \u0641\u064a \u0643\u0644 \u0645\u0627 \u062a\u0641\u0639\u0644\u0647.", "\u89e3\u66f0": "\u0647\u0630\u0647 \u0641\u0642\u0637. \u0627\u0644\u0631\u062c\u0644 \u0644\u0627 \u064a\u062e\u0637\u0626. \u0636\u0639\u0647\u0627 \u0641\u064a \u0627\u0639\u062a\u0628\u0627\u0631\u0643. \u0644\u0627 \u062a\u0643\u0646 \u0648\u062d\u064a\u062f\u0627.", "\u7c64\u8a69\u6545\u4e8b\u4e00": "\u25c7 \u064a\u0642\u0628\u0644 Dong Zhuo Lu Bu\n\u0643\u0627\u0646 \u0644\u0648 \u0628\u0648\u060c \u0627\u0644\u0634\u062c\u0627\u0639 \u0648\u0627\u0644\u062c\u064a\u062f \u0641\u064a \u0627\u0644\u0642\u062a\u0627\u0644\u060c \u062c\u0646\u0631\u0627\u0644\u0627\u064b \u0639\u0633\u0643\u0631\u064a\u0627\u064b \u0645\u0634\u0647\u0648\u0631\u0627\u064b \u0641\u064a \u0646\u0647\u0627\u064a\u0629 \u0623\u0633\u0631\u0629 \u0647\u0627\u0646 \u0627\u0644\u0634\u0631\u0642\u064a\u0629 \u0648\u0628\u062f\u0627\u064a\u0629 \u0627\u0644\u0645\u0645\u0627\u0644\u0643 \u0627\u0644\u062b\u0644\u0627\u062b. \u0648\u0643\u0627\u0646 \u0630\u0627\u062a \u064a\u0648\u0645 \u064a\u0639\u062a\u0628\u0631 \u062f\u064a\u0646\u063a \u064a\u0648\u0627\u0646\u060c \u062d\u0627\u0643\u0645 \u062c\u064a\u0646\u063a\u062a\u0634\u0648\u060c \u0623\u0628\u0627\u064b \u0628\u0627\u0644\u062a\u0628\u0646\u064a \u0648\u062e\u062f\u0645 \u062a\u062d\u062a \u0642\u064a\u0627\u062f\u062a\u0647.\n\u0628\u0639\u062f \u0648\u0641\u0627\u0629 \u0627\u0644\u0625\u0645\u0628\u0631\u0627\u0637\u0648\u0631 \u0644\u064a\u0646\u063a \u0645\u0646 \u0623\u0633\u0631\u0629 \u0647\u0627\u0646 \u0627\u0644\u0634\u0631\u0642\u064a\u0629\u060c \u0630\u0647\u0628 \u062f\u064a\u0646\u063a \u064a\u0648\u0627\u0646 \u0625\u0644\u0649 \u0628\u0643\u064a\u0646 \u0644\u0644\u062a\u0648\u0627\u0637\u0624 \u0645\u0639 \u0627\u0644\u062c\u0646\u0631\u0627\u0644 \u0647\u064a \u062c\u064a\u0646 \u0644\u0644\u0633\u064a\u0637\u0631\u0629 \u0639\u0644\u0649 \u0627\u0644\u062d\u0643\u0648\u0645\u0629.\n\u0642\u0627\u062a\u0644 Dong Zhuo \u0645\u0639 Ding Yuan\u060c \u0644\u0643\u0646 \u0627\u0644\u0639\u062f\u064a\u062f \u0645\u0646 \u0627\u0644\u062c\u0646\u0631\u0627\u0644\u0627\u062a \u0644\u0645 \u064a\u062a\u0645\u0643\u0646\u0648\u0627 \u0645\u0646 \u0647\u0632\u064a\u0645\u0629 Lu Bu\u060c \u0644\u0630\u0644\u0643 \u0623\u0631\u0633\u0644 Dong Zhuo \u0645\u0631\u0624\u0648\u0633\u0647 Li Su \u0644\u0631\u0634\u0648\u0629 Lu Bu. \u0623\u062d\u0636\u0631 \u0644\u064a \u0633\u0648 \u0623\u0643\u0648\u0627\u0645\u064b\u0627 \u0645\u0646 \u0627\u0644\u0643\u0646\u0648\u0632 \u0627\u0644\u0630\u0647\u0628\u064a\u0629 \u0648\u0627\u0644\u0641\u0636\u064a\u0629 \u0648\u062d\u0635\u0627\u0646 \u0627\u0644\u0623\u0631\u0646\u0628 \u0627\u0644\u0623\u062d\u0645\u0631 \u0627\u0644\u0634\u0647\u064a\u0631 \"\u0644\u0632\u064a\u0627\u0631\u0629 \u0644\u0648 \u0628\u0648\". \u0623\u062d\u0636\u0631 \u0644\u064a \u0633\u0648 \u0623\u0643\u0648\u0627\u0645\u064b\u0627 \u0645\u0646 \u0643\u0646\u0648\u0632 \u0627\u0644\u0630\u0647\u0628 \u0648\u0627\u0644\u0641\u0636\u0629 \u0648\u062d\u0635\u0627\u0646 \u0627\u0644\u0623\u0631\u0646\u0628 \u0627\u0644\u0623\u062d\u0645\u0631 \u0627\u0644\u0634\u0647\u064a\u0631 \u0644\u0632\u064a\u0627\u0631\u0629 \u0644\u0648 \u0628\u0648. \u0644\u0630\u0644\u0643 \u062a\u0644\u0642\u0649 \u0644\u0648 \u0628\u0648 \u0631\u0634\u0648\u0629 \u0645\u0646 \u062f\u0648\u0646\u063a \u062a\u0634\u0648 \u0644\u0642\u062a\u0644 \u062f\u064a\u0646\u063a \u064a\u0648\u0627\u0646\u060c \u0648\u0637\u0644\u0628 \u0645\u0646 \u062c\u0646\u0648\u062f\u0647 \u0623\u0646 \u064a\u0623\u062a\u0648\u0627 \u0645\u0639\u0647 \u0625\u0630\u0627 \u0623\u0631\u0627\u062f\u0648\u0627\u060c \u0648\u064a\u063a\u0627\u062f\u0631\u0648\u0627 \u0625\u0630\u0627 \u0643\u0627\u0646\u0648\u0627 \u0644\u0627 \u064a\u0631\u064a\u062f\u0648\u0646 \u0627\u062a\u0628\u0627\u0639\u0647. \u0648\u0646\u062a\u064a\u062c\u0629 \u0644\u0630\u0644\u0643\u060c \u0647\u0631\u0628 \u0645\u0639\u0638\u0645 \u0627\u0644\u062c\u0646\u0648\u062f. \u0644\u0630\u0644\u0643 \u062a\u0644\u0642\u0649 Lu Bu \u0631\u0634\u0648\u0629 \u0645\u0646 Dong Zhuo \u0648\u0642\u062a\u0644 Ding Yuan.\n\u0628\u0639\u062f \u0623\u0646 \u0627\u0643\u062a\u0633\u0628 Dong Zhuo \u0627\u0644\u0633\u0644\u0637\u0629\u060c \u062a\u0628\u0646\u0649 Lu Bu \u0628\u0627\u0639\u062a\u0628\u0627\u0631\u0647 \u0627\u0628\u0646\u0647 \u0628\u0627\u0644\u062a\u0628\u0646\u064a \u0648\u062c\u0639\u0644\u0647 \u062c\u0646\u0631\u0627\u0644\u064b\u0627 \u0648\u0645\u0627\u0631\u0643\u064a\u0632\u064b\u0627.\n\u0643\u0627\u0646 Dong Zhuo \u0645\u0632\u0627\u062c\u064b\u0627 \u0639\u0646\u064a\u0641\u064b\u0627\u060c \u0648\u0643\u0627\u0646\u062a \"\u0627\u0644\u0639\u0644\u0627\u0642\u0629 \u0628\u064a\u0646 \u0627\u0644\u0623\u0628 \u0648\u0627\u0644\u0627\u0628\u0646\" \u0645\u0639 Lu Bu \u0641\u064a \u0627\u0644\u0648\u0627\u0642\u0639 \u0636\u0639\u064a\u0641\u0629 \u062c\u062f\u064b\u0627\u060c \u0648\u0641\u064a \u0639\u0627\u0645 189 \u0645\u060c \u0628\u062a\u062d\u0631\u064a\u0636 \u0645\u0646 Situ Wang Yun\u060c \u0642\u062a\u0644 Lu Bu Dong Zhuo.\n\u0628\u0639\u062f \u0633\u062a\u0629 \u0623\u0634\u0647\u0631 \u0645\u0646 \u0648\u0641\u0627\u0629 \u062f\u0648\u0646\u063a \u062a\u0634\u0648\u060c \u063a\u0632\u062a \u0642\u0648\u0627\u062a \u062f\u0648\u0646\u063a \u062a\u0634\u0648 \u0627\u0644\u0642\u062f\u064a\u0645\u0629 \u0627\u0644\u0639\u0627\u0635\u0645\u0629\u060c \u0648\u0647\u0631\u0628 \u0644\u0648 \u0628\u0648 \u0648\u0632\u0648\u062c\u062a\u0647 \u0641\u064a \u062d\u0627\u0644\u0629 \u0645\u0646 \u0627\u0644\u0630\u0639\u0631. \u0628\u0639\u062f \u0645\u063a\u0627\u062f\u0631\u0629 \u0627\u0644\u0639\u0627\u0635\u0645\u0629\u060c \u0644\u062c\u0623 \u0644\u0648 \u0628\u0648 \u0625\u0644\u0649 \u0623\u0645\u0631\u0627\u0621 \u0627\u0644\u062d\u0631\u0628 \u064a\u0648\u0627\u0646 \u0634\u0648\u060c \u0648\u064a\u0648\u0627\u0646 \u0634\u0627\u0648\u060c \u0648\u062a\u0634\u0627\u0646\u063a \u064a\u0627\u0646\u063a\u060c \u0648\u0632\u0627\u0646\u063a \u0645\u064a\u0627\u0648\u060c \u0648\u0644\u064a\u0648 \u0628\u0627\u064a\u060c \u0648\u0645\u0627 \u0625\u0644\u0649 \u0630\u0644\u0643\u060c \u0648\u0642\u0627\u062a\u0644 \u0639\u062f\u0629 \u0645\u0631\u0627\u062a \u0645\u0639 \u0643\u0627\u0648 \u0643\u0627\u0648\u060c \u0648\u0647\u0648 \u0623\u0645\u064a\u0631 \u062d\u0631\u0628 \u0643\u0628\u064a\u0631 \u0622\u062e\u0631 \u0641\u064a \u0630\u0644\u0643 \u0627\u0644\u0648\u0642\u062a. \u0648\u0643\u0627\u0646 \u0644\u0648 \u0628\u0648 \u0634\u062c\u0627\u0639\u064b\u0627 \u0648\u062c\u064a\u062f\u064b\u0627 \u0641\u064a \u0627\u0644\u0642\u062a\u0627\u0644\u060c \u0648\u0647\u0632\u0645 \u0643\u0627\u0648 \u0643\u0627\u0648 \u0639\u062f\u0629 \u0645\u0631\u0627\u062a.\n\u0641\u064a \u0627\u0644\u0633\u0646\u0629 \u0627\u0644\u062b\u0627\u0644\u062b\u0629 \u0644\u062c\u064a\u0627\u0646\u0627\u0646 \u0641\u064a \u0623\u0633\u0631\u0629 \u0647\u0627\u0646 \u0627\u0644\u0634\u0631\u0642\u064a\u0629\u060c \u0647\u0627\u062c\u0645\u062a \u0642\u0648\u0627\u062a Cao Cao \u0642\u0627\u0639\u062f\u0629 Xiaopei\u060c \u0648\u062a\u0645 \u0623\u0633\u0631\u0647\u0627 \u0648\u0642\u062a\u0644\u0647\u0627 \u0639\u0644\u0649 \u064a\u062f Cao Cao \u062a\u062d\u062a \u0628\u0631\u062c Baimen. \u0642\u0628\u0644 \u0627\u0644\u0625\u0639\u062f\u0627\u0645\u060c \u0637\u0644\u0628 Lu Bu \u0645\u0646 Cao Cao \u0625\u0628\u0642\u0627\u0626\u0647 \u0639\u0644\u0649 \u0642\u064a\u062f \u0627\u0644\u062d\u064a\u0627\u0629 \u0644\u062a\u062c\u0646\u0628 \u0627\u0644\u0645\u0648\u062a\u060c \u0644\u0643\u0646 Liu Bei \u0630\u0643\u0651\u0631 Cao Cao \u0628\u0623\u0646 Lu Bu \u0642\u062f \u062e\u0627\u0646\u0647 \u0639\u062f\u0629 \u0645\u0631\u0627\u062a \u0648\u0643\u0627\u0646 \u0631\u062c\u0644\u0627\u064b \u063a\u064a\u0631 \u0623\u0645\u064a\u0646. \u0641\u064a \u0627\u0644\u0646\u0647\u0627\u064a\u0629\u060c \u0642\u0627\u0645 Cao Cao\u060c \u0627\u0644\u0630\u064a \u0643\u0627\u0646 \u064a\u0642\u062f\u0631 \u0627\u0644\u0645\u0648\u0627\u0647\u0628 \u0641\u0642\u0637 \u0628\u063a\u0636 \u0627\u0644\u0646\u0638\u0631 \u0639\u0646 \u0627\u0644\u0623\u062e\u0644\u0627\u0642\u060c \u0628\u0625\u0639\u062f\u0627\u0645 Lu Bu.", "\u8056\u610f": "\u0623\u0646\u0627 \u063a\u064a\u0631 \u0645\u0631\u062a\u0627\u062d \u0641\u064a \u0627\u0644\u0645\u0646\u0632\u0644\u060c \u0639\u0627\u062f\u0629\u064b \u0645\u0627 \u0623\u0628\u062d\u062b \u0639\u0646 \u0627\u0644\u0645\u0627\u0644\u060c \u0648\u0623\u062d\u0630\u0631 \u0645\u0646 \u0627\u0644\u0645\u0639\u0627\u0645\u0644\u0627\u062a\u060c \u0648\u0645\u0646 \u0627\u0644\u0635\u0639\u0628 \u0627\u0644\u0632\u0648\u0627\u062c\u060c \u0648\u064a\u062a\u0641\u0627\u062c\u0623 \u0631\u062c\u0644 Liujia\u060c \u0648\u064a\u0623\u062a\u064a \u0627\u0644\u0646\u0627\u0633 \u0625\u0644\u0649 \u0627\u0644\u062d\u0642\u0644\u060c \u0648\u062f\u0648\u062f\u0629 \u0627\u0644\u0642\u0632 \u0646\u0635\u0641 \u0646\u0627\u0636\u062c\u0629\u060c \u0648\u0633\u062a\u0629 \u062d\u064a\u0648\u0627\u0646\u0627\u062a \u0622\u0645\u0646\u0629\u060c \u0648\u0627\u0644\u0646\u0627\u0633 \u064a\u0628\u062d\u062b\u0648\u0646 \u0639\u0646 \u0627\u0644\u0646\u0627\u0633\u060c \u0627\u0644\u062f\u0639\u0627\u0648\u0649 \u0627\u0644\u0642\u0636\u0627\u0626\u064a\u0629 \u0645\u0646\u0627\u0633\u0628\u0629 \u0648\u0627\u0644\u0647\u062c\u0631\u0629\u060c \u0645\u0646 \u0627\u0644\u0635\u0639\u0628 \u0627\u0644\u0639\u062b\u0648\u0631 \u0639\u0644\u0649 \u0627\u0644\u0623\u0634\u064a\u0627\u0621 \u0627\u0644\u0645\u0641\u0642\u0648\u062f\u0629\u060c \u0648\u0645\u0646 \u0627\u0644\u0635\u0639\u0628 \u0627\u0644\u0639\u062b\u0648\u0631 \u0639\u0644\u0649 \u0627\u0644\u0623\u0645\u0631\u0627\u0636\u060c \u0648\u0647\u0646\u0627\u0643 \u0633\u0644\u0627\u0645 \u0641\u064a \u0645\u0642\u0628\u0631\u0629 \u062c\u0628\u0644 Zhenxing.", "\u53e4\u4eba": "\u064a\u0642\u0628\u0644 Dong Zhuo Lu Bu", "\u7b2cX\u7c64": "\u0627\u0644\u0641\u0627\u0626\u0632 \u0627\u0644\u0640 41 \u0641\u064a \u0627\u0644\u064a\u0627\u0646\u0635\u064a\u0628", "\u5bae": "\u0623\u064a\u0647\u0627 \u0627\u0644\u0642\u0635\u0631"}