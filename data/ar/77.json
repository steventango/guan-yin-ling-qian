{"\u7c64\u8a69\u7248\u672c\u4e8c": "\u0648\u0642\u064a\u0644 \u0641\u064a \u0627\u0644\u062d\u0644\u0645 \u0623\u0646\u0647 \u0633\u064a\u0643\u0648\u0646 \u0647\u0646\u0627\u0643 \u062b\u0631\u0648\u0629 \u0643\u062b\u064a\u0631\u0629\u060c \u0648\u0644\u0643\u0646 \u0627\u0644\u0634\u0647\u0631\u0629 \u0644\u0646 \u062a\u0630\u0647\u0628 \u0625\u0644\u0649 \u0634\u064a\u0621.\n\u0627\u0644\u0645\u0627\u0621 \u0628\u0639\u064a\u062f \u0648\u0627\u0644\u062c\u0628\u0627\u0644 \u0628\u0639\u064a\u062f\u0629 \u0644\u0643\u0646 \u0627\u0644\u0625\u062e\u0644\u0627\u0635 \u0635\u0639\u0628\u060c \u0627\u0644\u0625\u0646\u0633\u0627\u0646 \u0627\u0644\u0646\u0628\u064a\u0644 \u064a\u0647\u062f\u064a \u0648\u064a\u0628\u062a\u0633\u0645.", "\u8a69\u610f": "\u064a\u0645\u062b\u0644 \u0647\u0630\u0627 \u0627\u0644\u0634\u0643\u0644 \u0627\u0644\u0633\u062f\u0627\u0633\u064a \u0631\u0645\u0632\u064b\u0627 \u0644\u0644\u062d\u0635\u0648\u0644 \u0639\u0644\u0649 \u0643\u0646\u0632 \u0641\u064a \u0627\u0644\u062d\u0644\u0645.\n\u0643\u0644 \u0634\u064a\u0621 \u0623\u0643\u062b\u0631 \u0623\u0648 \u0623\u0642\u0644 \u0648\u0627\u0642\u0639\u064a\u0629.", "\u89e3\u66f0": "\u0627\u0644\u062d\u0644\u0645 \u0645\u062b\u0644 \u0627\u0644\u062d\u0644\u0645. \u0644\u0627 \u0623\u0645\u0644 \u0641\u064a \u0627\u0644\u0634\u0647\u0631\u0629. \u0641\u0642\u0637 \u0627\u0646\u062a\u0638\u0631 \u0627\u0644\u0623\u0648\u0642\u0627\u062a \u0627\u0644\u062c\u064a\u062f\u0629. \u062a\u0639\u0644\u064a\u0645\u0627\u062a \u0646\u0628\u064a\u0644\u0629", "\u7c64\u8a69\u6545\u4e8b\u4e00": "\u25c7 \u0625\u0639\u0627\u062f\u0629 \u0627\u0644\u064a\u0634\u0645 \u0625\u0644\u0649 \u062a\u0634\u0627\u0648 (\u0625\u0639\u0627\u062f\u0629 \u0627\u0644\u064a\u0634\u0645 \u0625\u0644\u0649 \u062a\u0634\u0627\u0648 \u0628\u0639\u062f \u0627\u0644\u0627\u0646\u062a\u0647\u0627\u0621 \u0645\u0646\u0647)\n\u0641\u064a \u0639\u0627\u0645 283 \u0642\u0628\u0644 \u0627\u0644\u0645\u064a\u0644\u0627\u062f\u060c \u062d\u0635\u0644 \u0627\u0644\u0645\u0644\u0643 Zhao Huiwen \u0639\u0644\u0649 He's Bi \u0645\u0646 Chu\u060c \u0648\u0628\u0639\u062f \u0623\u0646 \u0633\u0645\u0639 \u0627\u0644\u0645\u0644\u0643 Qin Zhaoxiang \u0628\u0627\u0644\u062e\u0628\u0631\u060c \u0623\u0631\u0633\u0644 \u062e\u0637\u0627\u0628\u064b\u0627 \u0625\u0644\u0649 \u0627\u0644\u0645\u0644\u0643 Zhao\u060c \u0645\u0639\u0631\u0628\u064b\u0627 \u0641\u064a\u0647 \u0639\u0646 \u0627\u0633\u062a\u0639\u062f\u0627\u062f\u0647 \u0644\u062a\u0628\u0627\u062f\u0644 \u062e\u0645\u0633\u0629 \u0639\u0634\u0631 \u0645\u062f\u064a\u0646\u0629 \u0645\u0642\u0627\u0628\u0644 He's Bi.\n\u062d\u0644\u0644 \u0644\u064a\u0646 \u0634\u064a\u0627\u0646\u063a\u0631\u0648 \u0644\u0644\u0645\u0644\u0643 \u062a\u0634\u0627\u0648 \u0623\u0646 \u062a\u0634\u064a\u0646 \u0643\u0627\u0646 \u0642\u0648\u064a\u064b\u0627 \u0648\u0623\u0646 \u062a\u0634\u0627\u0648 \u0643\u0627\u0646 \u0636\u0639\u064a\u0641\u064b\u0627 \u0648\u064a\u062c\u0628 \u0627\u0633\u062a\u0628\u062f\u0627\u0644\u0647.\n\u062b\u0645 \u0623\u0631\u0633\u0644 \u0627\u0644\u0645\u0644\u0643 Zhao Lin Xiangru \u0625\u0644\u0649 Qin \u0645\u0639 He's Bi.\n\u0627\u0644\u062a\u0642\u0649 \u0645\u0644\u0643 \u062a\u0634\u064a\u0646 \u0645\u0639 \u0644\u064a\u0646 \u0634\u064a\u0627\u0646\u063a\u0631\u0648 \u0641\u064a \u062a\u0634\u0627\u0646\u063a\u062a\u0627\u064a\u060c \u0648\u0642\u062f\u0645 \u0644\u064a\u0646 \u0634\u064a\u0627\u0646\u063a\u0631\u0648 \u0627\u0644\u064a\u0634\u0645 \u0625\u0644\u0649 \u0645\u0644\u0643 \u062a\u0634\u064a\u0646. \u0634\u0639\u0631 \u0645\u0644\u0643 \u062a\u0634\u064a\u0646 \u0628\u0633\u0639\u0627\u062f\u0629 \u063a\u0627\u0645\u0631\u0629 \u0648\u0645\u0631\u0631 He's Bi \u0625\u0644\u0649 \u0627\u0644\u062c\u0645\u064a\u0644\u0629 \u0648\u0646\u0638\u0631 \u0625\u0644\u064a\u0647\u0627 \u0643\u0644 \u0645\u0646 \u062d\u0648\u0644\u0647\u0627.\u0641\u064a \u0647\u0630\u0627 \u0627\u0644\u0648\u0642\u062a\u060c \u0635\u0631\u062e \u0627\u0644\u062c\u0645\u064a\u0639 \u0639\u0644\u0649 \u0627\u0644\u064a\u0633\u0627\u0631 \u0648\u0627\u0644\u064a\u0645\u064a\u0646\u060c \u062a\u062d\u064a\u0627.\n\u0631\u0623\u0649 \u0644\u064a\u0646 \u0634\u064a\u0627\u0646\u063a\u0631\u0648 \u0623\u0646 \u0645\u0644\u0643 \u062a\u0634\u064a\u0646 \u0644\u0645 \u064a\u0643\u0646 \u0644\u062f\u064a\u0647 \u0623\u064a \u0646\u064a\u0629 \u0644\u0625\u0639\u0637\u0627\u0621 \u0627\u0644\u0645\u062f\u064a\u0646\u0629 \u0625\u0644\u0649 \u062a\u0634\u0627\u0648\u060c \u0644\u0630\u0644\u0643 \u0623\u062e\u0628\u0631 \u0645\u0644\u0643 \u062a\u0634\u064a\u0646 \u0623\u0646 \u0627\u0644\u064a\u0634\u0645 \u0627\u0644\u062e\u0627\u0635 \u0628\u0647 \u0643\u0627\u0646 \u0645\u0639\u064a\u0628\u064b\u0627 \u0648\u0623\u0631\u0627\u062f \u0623\u0646 \u064a\u0634\u064a\u0631 \u0625\u0644\u0649 \u0645\u0644\u0643 \u062a\u0634\u064a\u0646 \u062d\u062a\u0649 \u064a\u0646\u062a\u0647\u0632 \u0627\u0644\u0641\u0631\u0635\u0629 \u0644\u0644\u062d\u0635\u0648\u0644 \u0639\u0644\u0649 \u0627\u0644\u064a\u0634\u0645 \u0645\u0631\u0629 \u0623\u062e\u0631\u0649.\n\u0628\u0639\u062f \u0623\u0646 \u0627\u0633\u062a\u0639\u0627\u062f \u0644\u064a\u0646 \u0634\u064a\u0627\u0646\u063a\u0631\u0648 \u0628\u064a \u0647\u064a\u0634\u064a\u060c \u0648\u0642\u0641 \u0628\u062c\u0648\u0627\u0631 \u0627\u0644\u0639\u0645\u0648\u062f \u0627\u0644\u0643\u0628\u064a\u0631\u060c \u0645\u0648\u0636\u062d\u064b\u0627 \u0623\u0646\u0647 \u064a\u0631\u064a\u062f \u0627\u0644\u0645\u0648\u062a \u0645\u0639 \u0647\u064a\u0634\u064a \u0628\u064a.\n\u0641\u064a \u0647\u0630\u0627 \u0627\u0644\u0648\u0642\u062a\u060c \u0642\u0627\u0644 \u0644\u064a\u0646 \u0634\u064a\u0627\u0646\u063a\u0631\u0648 \u0644\u0645\u0644\u0643 \u062a\u0634\u064a\u0646: \"\u0644\u0627 \u064a\u0632\u0627\u0644 \u0645\u0644\u0643 \u062a\u0634\u0627\u0648 \u064a\u062a\u063a\u0644\u0628 \u0639\u0644\u0649 \u0627\u0639\u062a\u0631\u0627\u0636\u0627\u062a \u0627\u0644\u0648\u0632\u0631\u0627\u0621 \u0648\u064a\u0642\u0628\u0644 \u0627\u0642\u062a\u0631\u0627\u062d \u0627\u0644\u0648\u0632\u064a\u0631. \u062d\u062a\u0649 \u0644\u0648 \u0644\u0645 \u064a\u062e\u062f\u0639 \u0628\u0639\u0636\u0647\u0645 \u0627\u0644\u0628\u0639\u0636 \u0639\u0646\u062f \u0627\u0644\u062a\u0641\u0627\u0639\u0644 \u0645\u0639 \u0639\u0627\u0645\u0629 \u0627\u0644\u0646\u0627\u0633\u060c \u0646\u0627\u0647\u064a\u0643 \u0639\u0646 \u0630\u0644\u0643 \u062a\u0634\u064a\u0646 \u062f\u0648\u0644\u0629 \u0643\u0628\u064a\u0631\u0629! \u0648\u0645\u0646 \u0623\u062c\u0644 \u0627\u0644\u062d\u0641\u0627\u0638 \u0639\u0644\u0649 \u0627\u0644\u0635\u062f\u0627\u0642\u0629 \u0628\u064a\u0646 \u062a\u0634\u064a\u0646 \u0648\u062a\u0634\u0627\u0648\u060c \u0623\u0631\u0633\u0644 \u0627\u0644\u0645\u0644\u0643 \u062a\u0634\u0627\u0648 \u0628\u0639\u062f \u0627\u0644\u0635\u064a\u0627\u0645 \u0644\u0645\u062f\u0629 \u062e\u0645\u0633\u0629 \u0623\u064a\u0627\u0645 \u0648\u0632\u0631\u0627\u0626\u064a \u0625\u0644\u0649 \u062a\u0634\u064a\u0646 \u0645\u0639 \u0627\u0644\u064a\u0634\u0645. \u0648\u0644\u0633\u0648\u0621 \u0627\u0644\u062d\u0638\u060c \u0644\u0645 \u064a\u0643\u0646 \u0644\u062f\u0649 \u0645\u0644\u0643 \u062a\u0634\u064a\u0646 \u0623\u064a \u0646\u064a\u0629 \u0644\u0625\u0639\u0637\u0627\u0621 \u062a\u0634\u0627\u0648 \u0627\u0644\u0645\u062f\u064a\u0646\u0629\u060c \u0648\u0644\u0643\u0646 \u0623\u064a\u0636\u064b\u0627 \u0645\u0631\u0631\u062a \u064a\u0634\u0645 \u0639\u0627\u0626\u0644\u0629 \u0647\u064a \u0625\u0644\u0649 \u0627\u0645\u0631\u0623\u0629 \u062c\u0645\u064a\u0644\u0629\u060c \u0627\u0644\u0623\u0645\u0631 \u0627\u0644\u0630\u064a \u0643\u0627\u0646 \u062d\u0642\u064b\u0627 \u0627\u0633\u062a\u0647\u0632\u0627\u0621\u064b \u0628\u0648\u0632\u0631\u0627\u0626\u064a.\"\n\u0628\u0639\u062f \u0627\u0644\u0627\u0646\u062a\u0647\u0627\u0621 \u0645\u0646 \u0627\u0644\u062d\u062f\u064a\u062b\u060c \u0642\u0627\u0645 \u0644\u064a\u0646 \u0634\u064a\u0627\u0646\u063a\u0631\u0648 \u0639\u0644\u0649 \u0627\u0644\u0641\u0648\u0631 \u0628\u0644\u0641\u062a\u0629 \u0636\u0631\u0628 \u0639\u0645\u0648\u062f.\u0643\u0627\u0646 \u0645\u0644\u0643 \u062a\u0634\u064a\u0646 \u062e\u0627\u0626\u0641\u064b\u0627 \u0645\u0646 \u0623\u0646 \u064a\u062a\u0636\u0631\u0631 \u0647\u0648 \u0628\u064a\u060c \u0644\u0630\u0644\u0643 \u062a\u0638\u0627\u0647\u0631 \u0628\u0625\u0639\u0637\u0627\u0621 \u062a\u0634\u0627\u0648 \u062e\u0645\u0633\u0629 \u0639\u0634\u0631 \u0645\u062f\u064a\u0646\u0629. \u0627\u0643\u062a\u0634\u0641 \u0644\u064a\u0646 \u0634\u064a\u0627\u0646\u063a\u0631\u0648 \u062e\u0637\u0629 \u0627\u0644\u0645\u0644\u0643 \u062a\u0634\u064a\u0646 \u0627\u0644\u063a\u0627\u062f\u0631\u0629\u060c \u0644\u0630\u0644\u0643 \u0637\u0644\u0628 \u0645\u0646 \u0627\u0644\u0645\u0644\u0643 \u062a\u0634\u064a\u0646 \u0623\u0646 \u064a\u0635\u0648\u0645 \u0644\u0645\u062f\u0629 \u062e\u0645\u0633\u0629 \u0623\u064a\u0627\u0645 \u0642\u0628\u0644 \u062a\u0642\u062f\u064a\u0645 He's Bi \u0644\u062a\u0623\u062e\u064a\u0631 \u0627\u0644\u0648\u0642\u062a. \u0641\u064a \u0647\u0630\u0647 \u0627\u0644\u0623\u064a\u0627\u0645 \u0627\u0644\u0642\u0644\u064a\u0644\u0629\u060c \u0623\u0631\u0633\u0644 \u0644\u064a\u0646 \u0634\u064a\u0627\u0646\u063a\u0631\u0648 \u0645\u0631\u0627\u0641\u0642\u064a\u0647 \u0644\u0644\u0639\u0648\u062f\u0629 \u0625\u0644\u0649 \u0648\u0644\u0627\u064a\u0629 \u062a\u0634\u0627\u0648 \u0645\u0639 \u0627\u0644\u064a\u0634\u0645\u060c \u0628\u064a\u0646\u0645\u0627 \u0628\u0642\u064a \u0647\u0648 \u0644\u0644\u062a\u0639\u0627\u0645\u0644 \u0645\u0639 \u0645\u0644\u0643 \u062a\u0634\u064a\u0646.\n(\u0647\u0630\u0647 \u0627\u0644\u0642\u0635\u0629 \u0645\u062b\u0644 \u0639\u064a\u0648\u0646 \u0634\u062e\u0635 \u0639\u0627\u062f\u064a \u0645\u0641\u062a\u0648\u0646\u0629 \u0628\u0645\u0643\u0627\u0646\u062a\u0647.)", "\u8056\u610f": "\u0627\u0644\u0635\u0644\u0627\u0629 \u0645\u0646 \u0623\u062c\u0644 \u0627\u0644\u0628\u0631\u0643\u0627\u062a \u0641\u064a \u0627\u0644\u0645\u0646\u0632\u0644\u060c \u0648\u0627\u0644\u0646\u0641\u0633\u060c \u0648\u0625\u0639\u0627\u0642\u0629 \u0627\u0644\u062b\u0631\u0648\u0629\u060c \u0648\u062c\u0639\u0644 \u0627\u0644\u0645\u0639\u0627\u0645\u0644\u0627\u062a \u0635\u0639\u0628\u0629 \u0627\u0644\u0625\u0646\u062c\u0627\u0632\u060c \u0648\u0627\u0644\u0632\u0648\u0627\u062c \u0644\u064a\u0633 \u0645\u062a\u0646\u0627\u063a\u0645\u064b\u0627 \u0645\u0639 \u0645\u0634\u0627\u0629 Liujiaxu\u060c \u0648\u0644\u0627 \u064a\u062a\u0645 \u0642\u0628\u0648\u0644 \u062f\u0648\u062f\u0629 \u0627\u0644\u0642\u0632 \u0627\u0644\u0634\u064a\u062a\u064a\u0627\u0646\u060c \u0648\u062a\u0644\u0641 \u0633\u062a\u0629 \u062d\u064a\u0648\u0627\u0646\u0627\u062a\u060c \u0648\u0627\u0644\u0623\u0634\u062e\u0627\u0635 \u0645\u0641\u0642\u0648\u062f\u0648\u0646\u060c \u0648\u0645\u0646 \u0627\u0644\u0645\u062a\u0648\u0642\u0639 \u0631\u0641\u0639 \u062f\u0639\u0627\u0648\u0649 \u0642\u0636\u0627\u0626\u064a\u0629 \u0639\u0627\u0645\u0629 \u0648\u0627\u0644\u0647\u062c\u0631\u0629\u060c \u0648\u0627\u0644\u0623\u0634\u064a\u0627\u0621 \u0627\u0644\u0645\u0641\u0642\u0648\u062f\u0629 \u0628\u0639\u064a\u062f\u0629\u060c \u0648\u062a\u0646\u0635\u0628 \u0627\u0644\u0623\u0645\u0631\u0627\u0636\u060c \u0648\u062a\u0631\u0633\u0644 \u0625\u0644\u0649 \u0627\u0644\u0645\u0642\u0627\u0628\u0631 \u0627\u0644\u062c\u0628\u0644\u064a\u0629\u060c \u0648\u062a\u062a\u063a\u064a\u0631.", "\u53e4\u4eba": "\u0623\u0639\u062f \u0627\u0644\u064a\u0634\u0645 \u0625\u0644\u0649 \u062a\u0634\u0627\u0648 (\u0623\u0643\u0645\u0644 \u0627\u0644\u064a\u0634\u0645 \u0625\u0644\u0649 \u062a\u0634\u0627\u0648)", "\u7b2cX\u7c64": "\u0627\u0644\u064a\u0627\u0646\u0635\u064a\u0628 \u0627\u0644\u0641\u0627\u0626\u0632 \u0627\u0644\u0633\u0627\u0628\u0639 \u0648\u0627\u0644\u0633\u0628\u0639\u0648\u0646", "\u5bae": "\u0642\u0635\u0631 \u0627\u0644\u0638\u0647\u0631"}