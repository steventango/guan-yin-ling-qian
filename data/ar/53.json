{"\u7c64\u8a69\u7248\u672c\u4e8c": "\u0639\u0646\u062f\u0645\u0627 \u064a\u062a\u062d\u0648\u0644 \u0627\u0644\u0625\u062d\u0628\u0627\u0637 \u0625\u0644\u0649 \u0627\u0628\u062a\u0647\u0627\u062c\u060c \u062a\u0632\u0623\u0631 \u0627\u0644\u062a\u0646\u0627\u0646\u064a\u0646 \u0648\u0632\u0626\u064a\u0631 \u0627\u0644\u0646\u0645\u0648\u0631.\n\u0627\u0644\u0633\u0645\u0627\u0621 \u0627\u0644\u0632\u0631\u0642\u0627\u0621 \u0644\u0647\u0627 \u0637\u0631\u064a\u0642\u0647\u0627 \u0627\u0644\u062e\u0627\u0635 \u0625\u0644\u0649 \u0627\u0644\u0633\u0645\u0627\u0621\u060c \u0647\u0644 \u0644\u064a \u0628\u0641\u0631\u0635\u0629 \u0623\u062e\u0631\u0649 \u0644\u062a\u062d\u0642\u064a\u0642 \u0627\u0644\u0634\u0647\u0631\u0629.", "\u8a69\u610f": "\u0647\u0630\u0627 \u0627\u0644\u0645\u062e\u0637\u0637 \u0627\u0644\u0633\u062f\u0627\u0633\u064a \u064a\u0634\u0628\u0647 \u0632\u0626\u064a\u0631 \u0627\u0644\u062a\u0646\u064a\u0646 \u0648\u0632\u0626\u064a\u0631 \u0627\u0644\u0646\u0645\u0631.\n\u0647\u0646\u0627\u0643 \u0623\u0645\u0644 \u0641\u064a \u0623\u0646 \u0643\u0644 \u0634\u064a\u0621 \u0633\u0648\u0641 \u064a\u0633\u064a\u0631 \u0639\u0644\u0649 \u0645\u0627 \u064a\u0631\u0627\u0645.", "\u89e3\u66f0": "\u0641\u0642\u0637 \u0642\u0645 \u0628\u0627\u0644\u062a\u062f\u0631\u064a\u0633 \u0648\u0627\u0630\u0647\u0628. \u0627\u062d\u0635\u0644 \u0639\u0644\u0649 \u0627\u0632\u062f\u0647\u0627\u0631\u0643 \u0627\u0644\u062e\u0627\u0635. \u064a\u062c\u0628 \u0623\u0646 \u064a\u062a\u0645 \u0641\u064a \u0627\u0644\u0646\u0647\u0627\u064a\u0629. \u0647\u0646\u0627\u0643 \u0645\u0643\u0627\u0646 \u064a\u0645\u0643\u0646 \u0627\u0644\u0639\u062b\u0648\u0631 \u0639\u0644\u064a\u0647.", "\u7c64\u8a69\u6545\u4e8b\u4e00": "\u25c7 \u064a\u0642\u0648\u0645 Liu Bei \u0628\u062a\u062c\u0646\u064a\u062f \u0627\u0644\u0639\u0631\u0648\u0633\n\u0628\u0639\u062f \u0645\u0639\u0631\u0643\u0629 \u062a\u0634\u064a\u0628\u064a\u060c \u0643\u0627\u0646 Sun Quan \u062d\u0631\u064a\u0635\u064b\u0627 \u0639\u0644\u0649 \u0627\u0633\u062a\u0639\u0627\u062f\u0629 Jingzhou\u060c \u0627\u0644\u062a\u064a \u0627\u0633\u062a\u0648\u0644\u0649 \u0639\u0644\u064a\u0647\u0627 Liu Bei\u060c \u0644\u0630\u0644\u0643 \u0627\u0633\u062a\u062e\u062f\u0645 \u062e\u0637\u0629 Zhou Yu\u060c \u0645\u062a\u0638\u0627\u0647\u0631\u064b\u0627 \u0628\u0623\u0646\u0647 \u064a\u0631\u064a\u062f \u062e\u0637\u0628\u0629 \u0623\u062e\u062a\u0647 Sun Ren \u0625\u0644\u0649 Liu Bei\u060c \u0644\u0643\u0646\u0647 \u0641\u064a \u0627\u0644\u0648\u0627\u0642\u0639 \u0623\u0631\u0627\u062f \u0627\u0644\u0627\u0633\u062a\u064a\u0644\u0627\u0621 \u0639\u0644\u064a\u0647\u0627. \u0644\u064a\u0648 \u0628\u0627\u064a \u0643\u0631\u0647\u064a\u0646\u0629 \u0644\u0644\u0645\u0637\u0627\u0644\u0628\u0629 \u0628\u062c\u064a\u0646\u063a\u062a\u0634\u0648.\n\u0628\u0639\u062f \u0623\u0646 \u062a\u062c\u0633\u0633 Zhuge Liang \u0639\u0644\u0649 \u062e\u0637\u0629 Zhou Yu\u060c \u0633\u0644\u0645 \u0627\u0644\u0643\u062a\u064a\u0628 \u0627\u0644\u0630\u064a \u064a\u062d\u062a\u0648\u064a \u0639\u0644\u0649 \u0627\u0644\u0625\u0633\u062a\u0631\u0627\u062a\u064a\u062c\u064a\u0629 \u0625\u0644\u0649 Zhao Yun\u060c \u0627\u0644\u0630\u064a \u0643\u0627\u0646 \u064a\u062d\u0645\u064a Liu Bei \u0639\u0646\u062f\u0645\u0627 \u0630\u0647\u0628 \u0625\u0644\u0649 Soochow. \u0627\u062a\u0628\u0639 Zhao Yun \u0627\u0644\u0625\u0633\u062a\u0631\u0627\u062a\u064a\u062c\u064a\u0629 \u0627\u0644\u0645\u0648\u062c\u0648\u062f\u0629 \u0641\u064a \u0627\u0644\u0643\u062a\u064a\u0628 \u0648\u0623\u062b\u0627\u0631 \u0639\u0645\u062f\u064b\u0627 \u0636\u062c\u0629 \u0643\u0628\u064a\u0631\u0629 \u0644\u0644\u0633\u0645\u0627\u062d \u0644\u0640 Wu Guotai ( \u0648\u0627\u0644\u062f \u0633\u0648\u0646 \u062a\u0634\u064a\u0648\u0627\u0646) \u0639\u0644\u0649 \u0639\u0644\u0645 \u0628\u0639\u0631\u0636 \u0627\u0644\u0632\u0648\u0627\u062c. \u0646\u0638\u0631\u064b\u0627 \u0644\u0623\u0646 Wu Guotai \u0623\u062d\u0628 \u0627\u0628\u0646\u062a\u0647 \u0648\u0644\u0645 \u064a\u0643\u0646 \u0645\u0633\u062a\u0639\u062f\u064b\u0627 \u0644\u062a\u0642\u0631\u064a\u0631 \u0634\u0624\u0648\u0646 \u0627\u0628\u0646\u062a\u0647\u0627 \u0645\u062f\u0649 \u0627\u0644\u062d\u064a\u0627\u0629 \u0628\u0634\u0643\u0644 \u0639\u0631\u0636\u064a\u060c \u0641\u0642\u062f \u0642\u0627\u0645\u062a \u0628\u062a\u0648\u0628\u064a\u062e Sun Quan \u0644\u0643\u0648\u0646\u0647 \u0642\u0627\u0633\u064a\u064b\u0627 \u0645\u0639 \u0623\u062e\u062a\u0647 \u0648\u0627\u0642\u062a\u0631\u062d\u062a \u0623\u0646 \u064a\u062e\u062a\u0628\u0631 \u0634\u062e\u0635\u064a\u064b\u0627 \u0645\u0627 \u0625\u0630\u0627 \u0643\u0627\u0646 Liu Bei \u064a\u0633\u062a\u062d\u0642 \u0627\u0628\u0646\u062a\u0647 \u0641\u064a \u0645\u0639\u0628\u062f Ganlu. \u0628\u0639\u062f \u0623\u0646 \u0627\u0644\u062a\u0642\u062a Wu Guotai \u0628\u0640 Liu Bei\u060c \u0623\u0639\u0631\u0628\u062a \u0639\u0646 \u062a\u0642\u062f\u064a\u0631\u0647\u0627 \u0627\u0644\u0643\u0628\u064a\u0631 \u0644\u0640 Liu Bei \u0648\u062f\u0639\u062a \u0628\u0634\u062f\u0629 \u0625\u0644\u0649 \u0627\u0644\u0632\u0648\u0627\u062c \u0628\u064a\u0646 Liu Bei \u0648\u0627\u0628\u0646\u062a\u0647\u060c \u0648\u0646\u062a\u064a\u062c\u0629 \u0644\u0630\u0644\u0643\u060c \u062a\u0632\u0648\u062c Liu Bei \u0628\u0646\u062c\u0627\u062d \u0645\u0646 \u0627\u0644\u0633\u064a\u062f\u0629 Sun.\n\u0644\u0627\u062d\u0642\u064b\u0627\u060c \u0647\u0631\u0628 Liu Bei \u0648\u0627\u0644\u0633\u064a\u062f\u0629 Sun \u0639\u0627\u0626\u062f\u064a\u0646 \u0625\u0644\u0649 Jingzhou \u0645\u0639\u064b\u0627. \u0644\u0627\u062d\u0642\u064b\u0627\u060c \u0623\u0631\u0627\u062f Sun Quan \u0627\u0644\u0627\u0633\u062a\u0641\u0627\u062f\u0629 \u0645\u0646 \u0647\u062c\u0648\u0645 Liu Bei \u0639\u0644\u0649 Yizhou \u0644\u0645\u0647\u0627\u062c\u0645\u0629 Jingzhou\u060c \u0644\u0643\u0646 Wu Guotai \u0623\u0648\u0642\u0641\u0647\u060c \u0627\u0644\u0630\u064a \u0643\u0627\u0646 \u0642\u0644\u0642\u064b\u0627 \u0639\u0644\u0649 \u0633\u0644\u0627\u0645\u0629 \u0627\u0628\u0646\u062a\u0647.\n\u0642\u0627\u0644: \"\u062e\u0637\u0629 \u062a\u0634\u0648 \u064a\u0648 \u0627\u0644\u0630\u0643\u064a\u0629 \u0647\u064a \u0627\u0644\u0623\u0641\u0636\u0644 \u0641\u064a \u0627\u0644\u0639\u0627\u0644\u0645. \u0644\u0642\u062f \u0641\u0642\u062f \u0632\u0648\u062c\u062a\u0647 \u0648\u0641\u0642\u062f \u0642\u0648\u0627\u062a\u0647.\"\n(\u0627\u0643\u062a\u0633\u0628 \u0644\u064a\u0648 \u0628\u0627\u064a \u0645\u0646 \u0627\u0644\u0623\u0630\u0649)", "\u8056\u610f": "\u0623\u0646\u0627 \u0633\u064a\u0621 \u0627\u0644\u062d\u0638 \u0641\u064a \u0627\u0644\u0645\u0646\u0632\u0644 \u0648\u0623\u0646\u0627 \u0633\u064a\u0621 \u0627\u0644\u062d\u0638 (\u0623\u0648\u0644 \u062d\u0638 \u0633\u064a\u0621\u060c \u062b\u0645 \u062d\u0638 \u0633\u0639\u064a\u062f). \u0627\u0628\u062d\u062b \u0639\u0646 \u0627\u0644\u062b\u0631\u0648\u0629\u060c \u0627\u0644\u062a\u0648\u0627\u0632\u0646\u060c \u0645\u0634\u0627\u0643\u0644 \u0641\u064a \u0627\u0644\u0632\u0648\u0627\u062c\u060c \u0627\u0644\u0646\u062c\u0627\u062d \u0641\u064a \u0627\u0644\u0632\u0648\u0627\u062c. \u0635\u0644\u0648\u0627 \u0645\u0646 \u0623\u062c\u0644 \u0627\u0644\u0645\u0627\u0631\u0629\u060c \u062f\u0648\u062f\u0629 \u0627\u0644\u0642\u0632 \u0641\u064a \u0627\u0644\u062d\u0642\u0648\u0644\u060c \u062e\u0645\u0633 \u0646\u0642\u0627\u0637 \u0644\u062a\u0644\u0641 \u0627\u0644\u0645\u0627\u0634\u064a\u0629\u060c \u0627\u0628\u062d\u062b \u0639\u0646 \u0627\u0644\u0646\u0627\u0633\u060c \u0627\u0644\u062f\u0639\u0627\u0648\u0649 \u0648\u0627\u0644\u0647\u062c\u0631\u0629\u060c \u0627\u0644\u062a\u0648\u0641\u064a\u0642 \u0644\u0644\u0636\u064a\u0627\u0639\u060c \u0627\u0644\u062a\u0648\u0641\u064a\u0642 \u0644\u0644\u0623\u0645\u0631\u0627\u0636\u060c \u0633\u0644\u0627\u0645 \u0627\u0644\u062c\u0628\u0627\u0644 \u0648\u0627\u0644\u0642\u0628\u0648\u0631.", "\u53e4\u4eba": "\u064a\u0642\u0648\u0645 \"\u0644\u064a\u0648 \u0628\u0627\u064a\" \u0628\u062a\u062c\u0646\u064a\u062f \u0639\u0631\u0648\u0633", "\u7b2cX\u7c64": "\u0627\u0644\u0631\u0642\u0645 \u0627\u0644\u0641\u0627\u0626\u0632 \u0627\u0644\u062b\u0627\u0644\u062b \u0648\u0627\u0644\u062e\u0645\u0633\u0648\u0646", "\u5bae": "\u0631\u064e\u062d\u0650\u0645"}