{"\u7c64\u8a69\u7248\u672c\u4e8c": "\u0639\u0646\u062f\u0645\u0627 \u064a\u0623\u062a\u064a \u0627\u0644\u0634\u062a\u0627\u0621\u060c \u0644\u0646 \u064a\u0647\u0644\u0643 \u063a\u0635\u0646 \u0632\u0647\u0631 \u0627\u0644\u0628\u0631\u0642\u0648\u0642 \u0639\u0644\u0649 \u0627\u0644\u062c\u0628\u0644\u060c \u062d\u062a\u0649 \u0644\u0648 \u0633\u0642\u0637\u062a \u0623\u0648\u0631\u0627\u0642\u0647 \u0648\u0630\u0628\u0644\u062a \u0623\u063a\u0635\u0627\u0646\u0647.\n\u0644\u0643\u0646 \u0639\u0646\u062f\u0645\u0627 \u064a\u0623\u062a\u064a \u0627\u0644\u0631\u0628\u064a\u0639\u060c \u0645\u0627\u0632\u0644\u062a \u0623\u0631\u063a\u0628 \u0641\u064a \u0623\u0646 \u0623\u0643\u0648\u0646 \u0627\u0644\u0645\u062d\u0638\u064a\u0629 \u0648\u0623\u062f\u0645\u0631 \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u0623\u0635\u0644\u064a.", "\u8a69\u610f": "\u0641\u064a \u0647\u0630\u0627 \u0627\u0644\u0645\u062e\u0637\u0637 \u0627\u0644\u0633\u062f\u0627\u0633\u064a\u060c \u062a\u062d\u062a\u0644 \u0632\u0647\u0631\u0629 \u0627\u0644\u0628\u0631\u0642\u0648\u0642 \u0645\u0643\u0627\u0646\u0629 \u0631\u0627\u0626\u062f\u0629.\n\u0643\u0644 \u0634\u064a\u0621 \u062c\u064a\u062f \u0625\u0630\u0627 \u062a\u0623\u062e\u0631.", "\u89e3\u66f0": "\u0637\u0644\u0642\u0629 \u0633\u0647\u0645 \u0648\u0627\u062d\u062f\u0629 \u0641\u0627\u0631\u063a\u0629. \u0639\u0646\u062f\u0645\u0627 \u062a\u0643\u0648\u0646 \u0641\u0627\u0631\u063a\u0629\u060c \u0641\u0647\u064a \u0644\u064a\u0633\u062a \u0641\u0627\u0631\u063a\u0629. \u0641\u064a \u0627\u0646\u062a\u0638\u0627\u0631 \u0642\u062f\u0648\u0645 \u0627\u0644\u0631\u0628\u064a\u0639. \u0627\u0644\u0644\u0648\u0646 \u0641\u064a\u0647.", "\u7c64\u8a69\u6545\u4e8b\u4e00": "\u25c7\u0633\u062c\u0644 \u0647\u062f\u0641\u064a\u0646\n\u0641\u064a \u0639\u0647\u062f \u0627\u0644\u0625\u0645\u0628\u0631\u0627\u0637\u0648\u0631 Dezong \u0645\u0646 \u0623\u0633\u0631\u0629 Tang\u060c \u062a\u0645 \u062a\u0648\u0631\u064a\u0637 \u0627\u0644\u0648\u0632\u064a\u0631 \u0627\u0644\u0645\u062e\u0644\u0635 Mei Bogao \u0648\u0639\u0627\u0626\u0644\u062a\u0647 \u0645\u0646 \u0642\u0628\u0644 \u0631\u0626\u064a\u0633 \u0627\u0644\u0648\u0632\u0631\u0627\u0621 \u0627\u0644\u062e\u0627\u0626\u0646 Lu Qi\u060c \u0627\u0644\u0630\u064a \u0623\u0631\u0627\u062f \u0642\u062a\u0644\u0647\u0645 \u062c\u0645\u064a\u0639\u064b\u0627\u060c \u0648\u0644\u0645 \u064a\u062a\u0645\u0643\u0646 \u0633\u0648\u0649 \u0627\u0628\u0646\u0647 Mei Liangyu \u0645\u0646 \u0627\u0644\u0641\u0631\u0627\u0631\u060c \u0648\u0641\u064a \u0648\u0642\u062a \u0644\u0627\u062d\u0642 \u0642\u0627\u0645 \u0628\u062a\u063a\u064a\u064a\u0631 \u0627\u0633\u0645\u0647. \u0627\u0633\u0645 Wang Xitong \u0644\u0625\u062e\u0641\u0627\u0621 \u0647\u0648\u064a\u062a\u0647. \u0643\u0627\u0646 \u064a\u0639\u064a\u0634 \u0648\u062d\u064a\u062f\u064b\u0627 \u0641\u064a \u0627\u0644\u0634\u0648\u0627\u0631\u0639\u060c \u062c\u0627\u0626\u0639\u064b\u0627 \u0648\u0628\u0627\u0631\u062f\u064b\u0627\u060c \u0644\u0643\u0646 \u0644\u062d\u0633\u0646 \u0627\u0644\u062d\u0638 \u0627\u0633\u062a\u0642\u0628\u0644\u0647 \u062a\u0634\u064a\u0646 \u0631\u064a\u0634\u0646\u063a (\u062a\u0634\u0646 \u062f\u0648\u0646\u063a\u062a\u0634\u0648). \u0644\u0645 \u064a\u0643\u0646 \u062a\u0634\u064a\u0646 \u0631\u064a\u0634\u064a\u0646\u063a \u064a\u0639\u0631\u0641 \u062a\u062c\u0631\u0628\u062a\u0647 \u0627\u0644\u062d\u064a\u0627\u062a\u064a\u0629\u060c \u0644\u0630\u0627 \u0637\u0644\u0628 \u0645\u0646\u0647 \u0627\u0644\u0645\u0633\u0627\u0639\u062f\u0629 \u0641\u064a \u0631\u0639\u0627\u064a\u0629 \u062d\u062f\u064a\u0642\u0629 \u0627\u0644\u0628\u0631\u0642\u0648\u0642 \u0627\u0644\u062e\u0627\u0635\u0629 \u0628\u062a\u0634\u0646 \u062d\u062a\u0649 \u0644\u0627 \u064a\u0645\u0648\u062a \u062c\u0648\u0639\u064b\u0627 \u0641\u064a \u0627\u0644\u0634\u0627\u0631\u0639.\n\u0643\u0627\u0646 \u062a\u0634\u064a\u0646 \u0631\u064a\u0634\u0646\u063a \u0632\u0645\u064a\u0644\u064b\u0627 \u0648\u0635\u062f\u064a\u0642\u064b\u0627 \u0644\u0640 Mei Bogao\u060c \u0648\u0639\u0646\u062f\u0645\u0627 \u062a\u0639\u0631\u0636 \u0644\u0644\u0627\u0636\u0637\u0647\u0627\u062f \u0645\u0646 \u0642\u0628\u0644 Lu Qi\u060c \u0623\u0646\u0642\u0630\u0647 Mei Bogao\u060c \u0648\u0646\u062a\u064a\u062c\u0629 \u0644\u0630\u0644\u0643\u060c \u0623\u0635\u0628\u062d\u062a Mei Bogao \u0648Lu Qi \u0641\u064a \u0639\u062f\u0627\u0648\u0629\u060c \u0648\u0641\u064a \u0627\u0644\u0646\u0647\u0627\u064a\u0629\u060c \u062a\u0645 \u0625\u0639\u062f\u0627\u0645 \u0639\u0627\u0626\u0644\u062a\u0647\u0645\u0627 \u0628\u0623\u0643\u0645\u0644\u0647\u0627. \u0634\u0639\u0631 \u0631\u064a\u0634\u064a\u0646\u063a \u0628\u0627\u0644\u062d\u0632\u0646 \u0648\u0627\u0644\u063a\u0636\u0628 \u0628\u0633\u0628\u0628 \u0630\u0644\u0643\u060c \u0648\u0628\u0639\u062f \u0623\u0646 \u062a\u0632\u0648\u062c \u0627\u0633\u062a\u0642\u0627\u0644 \u0648\u0627\u0646\u062a\u0642\u0644 \u0628\u0623\u0633\u0631\u062a\u0647 \u0625\u0644\u0649 \u064a\u0627\u0646\u063a\u062a\u0634\u0648 \u0644\u064a\u0639\u064a\u0634 \u0641\u064a \u0639\u0632\u0644\u0629.\n\u062a\u0634\u064a\u0646 \u0631\u064a\u0634\u0646\u063a \u0644\u062f\u064a\u0647 \u0627\u0628\u0646\u0629 \u062a\u062f\u0639\u0649 \u062a\u0634\u064a\u0646 \u0634\u064a\u0646\u063a \u064a\u0648\u0627\u0646\u060c \u0648\u0647\u064a \u062c\u0645\u064a\u0644\u0629 \u0648\u0639\u0637\u0631\u0629\u060c \u0628\u0639\u062f \u0623\u0646 \u062c\u0627\u0621\u062a \u0645\u064a \u0644\u064a\u0627\u0646\u063a\u064a\u0648 \u0625\u0644\u0649 \u0639\u0627\u0626\u0644\u0629 \u062a\u0634\u064a\u0646\u060c \u0648\u0642\u0639\u0648\u0627 \u0641\u064a \u0627\u0644\u062d\u0628 \u0645\u0646 \u0627\u0644\u0646\u0638\u0631\u0629 \u0627\u0644\u0623\u0648\u0644\u0649. \u0641\u064a \u0627\u0644\u064a\u0648\u0645 \u0627\u0644\u0630\u064a \u0643\u0627\u0646\u062a \u0641\u064a\u0647 \u0623\u0632\u0647\u0627\u0631 \u0627\u0644\u0628\u0631\u0642\u0648\u0642 \u0641\u064a \u062d\u0627\u0644\u0629 \u0625\u0632\u0647\u0627\u0631 \u0643\u0627\u0645\u0644\u060c \u0648\u0627\u0644\u0630\u064a \u0643\u0627\u0646 \u0623\u064a\u0636\u064b\u0627 \u0630\u0643\u0631\u0649 \u0648\u0641\u0627\u0629 \u0645\u064a \u0628\u0648\u063a\u0627\u0648\u060c \u0641\u062a\u062d \u062a\u0634\u064a\u0646 \u0631\u064a\u0634\u0646\u063a \u0648\u0639\u0627\u0626\u0644\u062a\u0647 \u0645\u0630\u0628\u062d\u064b\u0627 \u0641\u064a \u062d\u062f\u064a\u0642\u0629 \u0627\u0644\u0628\u0631\u0642\u0648\u0642 \u0641\u064a \u0645\u0646\u0632\u0644\u0647\u0645 \u0644\u0625\u062d\u064a\u0627\u0621 \u0630\u0643\u0631\u0649 \u0635\u062f\u064a\u0642\u0647\u0645 \u0627\u0644\u0645\u062a\u0648\u0641\u0649 \u0648\u0627\u0644\u0645\u062d\u0633\u0646. \u0628\u0634\u0643\u0644 \u063a\u064a\u0631 \u0645\u062a\u0648\u0642\u0639\u060c \u062a\u063a\u064a\u0631 \u0627\u0644\u0637\u0642\u0633 \u0641\u062c\u0623\u0629 \u060c \u0648\u0647\u0628\u062a \u0639\u0627\u0635\u0641\u0629 \u062b\u0644\u062c\u064a\u0629 \u0634\u062f\u064a\u062f\u0629\u060c \u063a\u0637\u062a \u0643\u0644 \u0623\u0632\u0647\u0627\u0631 \u0627\u0644\u0628\u0631\u0642\u0648\u0642. \u0623\u0639\u0631\u0628 \u062a\u0634\u064a\u0646 \u0631\u064a\u0634\u0646\u063a \u0639\u0646 \u0623\u0633\u0641\u0647 \u0644\u0623\u0646\u0647 \u062a\u0645\u0643\u0646 \u0645\u0646 \u0627\u0644\u0647\u0631\u0648\u0628 \u0628\u0623\u0645\u0627\u0646\u060c \u0644\u0643\u0646\u0647 \u0644\u0645 \u064a\u062a\u0645\u0643\u0646 \u062d\u062a\u0649 \u0645\u0646 \u0625\u0646\u0642\u0627\u0630 \u0623\u062d\u0641\u0627\u062f \u0639\u0627\u0626\u0644\u0629 \u0645\u064a\u060c \u0648\u0627\u0639\u062a\u0642\u062f \u0623\u0646 \u0647\u0630\u0627 \u062a\u062d\u0630\u064a\u0631 \u0645\u0646 \u0627\u0644\u0644\u0647\u060c \u0644\u0630\u0644\u0643 \u0623\u0642\u0633\u0645 \u0623\u0646\u0647 \u0645\u0627 \u0644\u0645 \u062a\u062a\u0641\u062a\u062d \u0623\u0632\u0647\u0627\u0631 \u0627\u0644\u0628\u0631\u0642\u0648\u0642 \u0645\u0631\u0629 \u0623\u062e\u0631\u0649\u060c \u0645\u0645\u0627 \u064a\u062b\u0628\u062a \u0623\u0646 \u0623\u062d\u0641\u0627\u062f \u0643\u0627\u0646\u062a \u0639\u0627\u0626\u0644\u0629 \u0645\u064a \u0644\u0627 \u062a\u0632\u0627\u0644 \u0639\u0644\u0649 \u0642\u064a\u062f \u0627\u0644\u062d\u064a\u0627\u0629\u060c \u0648\u0644\u0645 \u064a\u0643\u0646 \u0644\u064a\u0642\u0648\u0644 \u0623\u064a \u0634\u064a\u0621 \u0623\u0628\u062f\u064b\u0627\u060c \u0648\u0642\u062f \u0647\u0631\u0628 \u0625\u0644\u0649 \u0627\u0644\u0628\u0648\u0630\u064a\u0629 \u0648\u0623\u0635\u0628\u062d \u0631\u0627\u0647\u0628\u064b\u0627.\n\u0633\u0645\u0639\u062a \u0627\u0628\u0646\u062a\u0647 \u0634\u064a\u0646\u063a \u064a\u0648\u0627\u0646 \u0630\u0644\u0643 \u0648\u0623\u062d\u0631\u0642\u062a \u0627\u0644\u0628\u062e\u0648\u0631 \u0639\u0644\u0649 \u0627\u0644\u0641\u0648\u0631 \u0648\u0635\u0644\u0651\u062a \u0628\u0625\u062e\u0644\u0627\u0635 \u0625\u0644\u0649 \u0627\u0644\u0644\u0647 \u0623\u0646 \u064a\u062c\u0639\u0644 \u0623\u0632\u0647\u0627\u0631 \u0627\u0644\u0628\u0631\u0642\u0648\u0642 \u062a\u062a\u0641\u062a\u062d \u0645\u0631\u0629 \u0623\u062e\u0631\u0649. \u0644\u0642\u062f \u062d\u0631\u0643 \u0635\u062f\u0642\u0647\u0645 \u0627\u0644\u0633\u0645\u0627\u0621\u060c \u0648\u0623\u0632\u0647\u0631\u062a \u062c\u0645\u064a\u0639 \u0623\u0634\u062c\u0627\u0631 \u0627\u0644\u0628\u0631\u0642\u0648\u0642 \u0641\u064a \u062d\u062f\u064a\u0642\u0629 \u0627\u0644\u0628\u0631\u0642\u0648\u0642 \u0645\u0631\u0629 \u0623\u062e\u0631\u0649\u061b \u0644\u0627\u062d\u0642\u064b\u0627 \u0639\u0631\u0641 \u062a\u0634\u064a\u0646 \u0631\u064a\u0634\u0646\u063a \u0623\u0646 Xitong \u0647\u0648 Liangyu\u060c \u0644\u0630\u0644\u0643 \u062e\u0637\u0628 \u0627\u0628\u0646\u062a\u0647 Xingyuan \u0644\u0647.", "\u8056\u610f": "\u0644\u0627 \u064a\u0635\u0644\u062d \u0627\u0644\u0628\u064a\u062a \u0648\u0627\u0644\u0628\u064a\u062a \u0644\u0646\u0641\u0633\u0647\u060c \u0648\u0644\u0627 \u0644\u062e\u064a\u0631\u0647\u060c \u0648\u0644\u0627 \u0644\u0637\u0644\u0628 \u0627\u0644\u0645\u0627\u0644\u060c \u0648\u0644\u0627 \u0644\u0644\u062d\u0643\u0645\u0629 \u0641\u064a \u0627\u0644\u0645\u0639\u0627\u0645\u0644\u0627\u062a\u060c \u0648\u0644\u0627 \u0644\u0644\u0627\u0646\u062a\u0638\u0627\u0631 \u062d\u062a\u0649 \u0648\u0642\u062a \u0627\u0644\u0632\u0648\u0627\u062c\u060c \u0648\u0644\u0627 \u0644\u0644\u062a\u0623\u062e\u0631\u060c \u0648\u0644\u0644\u0632\u0648\u0627\u062c\u060c \u0648\u0644\u0644\u062a\u0623\u062e\u0631\u060c \u0648\u0644\u0644\u0641\u0644\u0627\u062d \u0641\u064a \u0627\u0644\u062f\u0646\u064a\u0627. \u0627\u0644\u0631\u0628\u064a\u0639\u060c \u0623\u0646 \u064a\u062a\u0623\u062e\u0631\u060c \u0623\u0646 \u064a\u062a\u0623\u062e\u0631\u060c \u0623\u0646 \u064a\u062a\u0623\u062e\u0631\u060c \u0623\u0646 \u064a\u0643\u0648\u0646 \u0645\u0634\u063a\u0648\u0644\u0627\u064b\u060c \u0623\u0646 \u064a\u0643\u0648\u0646 \u0645\u0634\u063a\u0648\u0644\u0627\u064b\u060c \u0623\u0646 \u064a\u0643\u0648\u0646 \u0645\u0632\u062f\u0647\u0631\u0627\u064b\u060c \u0623\u0646 \u064a\u0643\u0648\u0646 \u0645\u0634\u063a\u0648\u0644\u0627\u064b\u060c \u0623\u0646 \u064a\u062a\u0645 \u0627\u0644\u0639\u062b\u0648\u0631 \u0639\u0644\u064a\u0647.", "\u53e4\u4eba": "\u0633\u062c\u0644 \u0647\u062f\u0641\u064a\u0646", "\u7b2cX\u7c64": "\u062a\u0633\u0639\u0629 \u0648\u0633\u062a\u0648\u0646 \u0627\u0644\u0641\u0648\u0632 \u0628\u0627\u0644\u0644\u0648\u062a", "\u5bae": "\u062a\u0627\u062a\u0633\u0648\u0645\u064a"}