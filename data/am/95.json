{"\u7c64\u8a69\u7248\u672c\u4e8c": "\u121d\u129e\u1271 \u12a5\u1293 \u1235\u12ac\u1276\u127d \u1260\u1235\u122d\u12c8-\u1218\u1295\u130d\u1235\u1275 \u12cd\u1235\u1325 \u1293\u1278\u12cd, \u12a0\u1201\u1295 \u130d\u1295 \u121b\u1208\u1242\u12eb \u12e8\u120c\u1208\u12cd \u1218\u1320\u1325 \u12a5\u1293 \u12c8\u1232\u1265 \u12ed\u1296\u122b\u1209.\n\u12c8\u122d\u1243\u121b \u12f6\u122e \u12ab\u12e8\u1205 \u1308\u1295\u12d8\u1265\u1363 \u1200\u1265\u1275\u1293 \u1200\u1265\u1275 \u1275\u1238\u120d\u121b\u1208\u1205\u1362", "\u8a69\u610f": "\u12ed\u1205 \u1204\u12ad\u1233\u130d\u122b\u121d \u12e8\u134d\u120b\u130e\u1275 \u12a5\u1293 \u12e8\u12dd\u1293 \u121d\u120d\u12ad\u1275\u1295 \u12ed\u12c8\u12ad\u120b\u120d\u1362\n\u1201\u1209\u1295\u121d \u1290\u1308\u122d \u1260\u1325\u1229 \u1201\u1294\u1273 \u12eb\u1246\u12e9\u1362", "\u89e3\u66f0": "\u1309\u1309 \u12a5\u1293 \u1309\u1309\u1275\u1362 \u12a5\u122b\u1235\u12ce\u1295 \u1208\u1218\u1320\u1260\u1245 \u130a\u12dc\u12cd \u12a0\u120d\u1228\u1348\u12f0\u121d. \u12c8\u122d\u1243\u121b\u12cd \u12f6\u122e \u1243\u120d \u1308\u1265\u1277\u120d. \u1201\u1209\u121d \u12f0\u1235\u1270\u129b\u1362", "\u7c64\u8a69\u6545\u4e8b\u4e00": "\u25c7\u12ab\u12a6 \u1352 \u122b\u1231\u1295 \u1295\u1309\u1220 \u1290\u1308\u1225\u1275 \u12a0\u12f5\u122d\u130e \u12a0\u12c8\u1300\n\u12ab\u12a6 \u1352\u1363 \u1260\u12a0\u12ad\u1265\u122e\u1275 \u1235\u121d \u12da\u1201\u12cb\u1295\u1363 \u12e8\u1270\u12c8\u1208\u12f0\u12cd \u1260\u1354\u1309\u12a6\u12aa\u12eb\u12a6 (\u12a0\u1201\u1295 \u1203\u12a6 \u12ab\u12cd\u1295\u1272\u1363 \u12a0\u1295\u1201\u12ea) \u1290\u12cd\u1362 \u1260\u1302\u12eb\u1295 \u12d8\u1218\u1295 \u1273\u12cb\u1242 \u1356\u1208\u1272\u12a8\u129b \u12a5\u1293 \u1338\u1210\u134a\u1362\n\u12ab\u12a6 \u1352 \u12e8\u12ab\u12a6 \u12ab\u12a6 \u1201\u1208\u1270\u129b \u120d\u1305 \u1290\u12cd \u12a8\u120d\u1305\u1290\u1271 \u1300\u121d\u122e \u12a8\u12a0\u1263\u1271 \u130b\u122d \u1260\u12c8\u1273\u12f0\u122b\u12ca \u12ab\u121d\u1355 \u12ed\u1296\u122d \u1290\u1260\u122d \u1260\u12a0\u1263\u1271 \u1270\u1345\u12a5\u1296 \u12ab\u12a6 \u1352 \u12606 \u12a0\u1218\u1271 \u1240\u1235\u1275 \u12cd\u122d\u12c8\u122b \u12a5\u1293 \u12e8\u1348\u1228\u1235 \u130d\u120d\u1262\u12eb\u1295 \u1260\u1235\u121d\u1295\u1275 \u12a0\u1218\u1271 \u1270\u121d\u122f\u120d \u12a5\u1293 \u12a0\u12f0\u1308\u1362 \u1325\u1229 \u12e8\u121b\u122d\u123b\u120d \u12a0\u122d\u1275 \u127d\u120e\u1273\u1362 \u1260\u1270\u1328\u121b\u122a\u121d \u1235\u1208 \u1325\u1295\u1273\u12ca \u12a5\u1293 \u12d8\u1218\u1293\u12ca \u12ad\u120b\u1232\u12ae\u127d \u12a5\u1293 \u12e8\u1205\u12ed\u12c8\u1275 \u1273\u122a\u12ae\u127d \u12a5\u1295\u12f2\u1201\u121d \u1260\u1218\u1276\u12ce\u127d \u12e8\u121a\u1246\u1320\u1229 \u12e8\u12a0\u1235\u1270\u1233\u1230\u1265 \u1275\u121d\u1205\u122d\u1275 \u1264\u1276\u127d \u1218\u133d\u1203\u134e\u127d\u1295 \u1260\u1270\u1218\u1208\u12a8\u1270 \u1230\u134a \u12a5\u12cd\u1240\u1275 \u12a0\u1208\u12cd. \u12ab\u12a6 \u12ab\u12a6 \u12e9\u12cb\u1295 \u123b\u12a6\u1295 \u12eb\u1238\u1290\u1348\u12cd \u12a5\u1235\u12a8 \u12a0\u1235\u122b \u1235\u121d\u1295\u1275 \u12a0\u1218\u1271 \u12f5\u1228\u1235 \u1290\u1260\u122d \u12a5\u1293 \u12ab\u12a6 \u1352 \u12e8\u12ab\u12a6 \u12ab\u12a6 \u12a0\u1308\u12db\u12dd \u121b\u12d5\u12a8\u120d \u1260\u1206\u1290\u127d\u12cd \u1260\u12ec\u127c\u1295\u130d \u1260\u12a0\u1295\u1343\u122b\u12ca\u1290\u1275 \u12e8\u1270\u1228\u130b\u130b \u1205\u12ed\u12c8\u1275 \u1296\u1228\u1362 \u12a8\u12da\u12eb\u1295 \u130a\u12dc \u1300\u121d\u122e \u121d\u1295\u121d \u12a5\u1295\u12b3\u1295 \u12a0\u1295\u12f3\u1295\u12f5 \u130a\u12dc \u12a8\u12a0\u1263\u1271 \u130b\u122d \u1260\u12c8\u1273\u12f0\u122b\u12ca \u1309\u12de\u12ce\u127d \u12cd\u1235\u1325 \u1262\u1204\u12f5 \u12a5\u1293 \u12a0\u1265\u12db\u129b\u12cd\u1295 \u130a\u12dc \u12e8\u1270\u12a8\u1260\u1228\u12cd\u1295 \u120d\u1305 \u1205\u12ed\u12c8\u1275 \u1262\u1218\u122b\u121d, \u1260\u12e8\u1240\u1291 \u121b\u122d\u123b\u120d \u12a0\u122d\u1275 \u12ed\u1208\u121b\u1218\u12f3\u120d \u12a5\u1293 \u12a5\u1235\u12a8 \u12db\u122c \u12f5\u1228\u1235 \u12a0\u120d\u1240\u1290\u1230\u121d. \u1265\u12d9 \u130a\u12dc \u1260\u1348\u1228\u1235 \u120b\u12ed \u12e8\u121a\u130b\u120d\u1260\u12cd \u1218\u1276 \u12a5\u122d\u121d\u1303 \u1232\u1206\u1295 \u1260\u1323\u121d \u12e8\u1270\u12ab\u1290 \u1290\u12cd\u1361 \u1265\u12d9 \u130a\u12dc \u12a9\u1295\u130d\u1349\u1295 \u1235\u1208\u121a\u1208\u121b\u1218\u12f5\u1361 \u1240\u120b\u120d \u12a5\u1293 \u1320\u1295\u12ab\u122b \u1290\u12cd\u1362\n\u12e8\u12ab\u12a6 \u12ab\u12a6 \u12e8\u1260\u12a9\u122d \u120d\u1305 \u12ab\u12a6 \u12a0\u1295\u130d \u1260\u1326\u122d\u1290\u1275 \u1270\u1308\u12f0\u1208\u1362 \u12e8\u1260\u12a9\u122d \u120d\u1305 \u12a8\u121e\u1270 \u1260\u128b\u120b, \u12ab\u12a6 \u1352 \u1208\u12d8\u12cd\u12f5 \u120d\u12d1\u120d \u12e8\u1218\u1300\u1218\u122a\u12eb \u121d\u122d\u132b \u1206\u1290. \u1220\u120b\u1233 \u12a0\u1295\u12f5 \u12d3\u1218\u1275 \u1232\u1206\u1290\u12cd \u1260\u12ab\u12a6 \u12ab\u12a6 \u12d8\u12cd\u12f5 \u120d\u12d1\u120d \u1270\u12f0\u1228\u1308\u1362 Cao Pi \u12e8\u1220\u120b\u1233 \u1226\u1235\u1275 \u12d3\u1218\u1275 \u120d\u1305 \u12a5\u12eb\u1208\u1363 \u12ab\u12a6 \u12ab\u12a6 \u1260\u1205\u1218\u121d \u121e\u1270\u1363 \u12a5\u1293 \u12ab\u12a6 \u1352 \u1260\u1320\u1245\u120b\u12ed \u121a\u1292\u1235\u1275\u122d\u1290\u1275 \u12a5\u1293 \u12e8\u12cb\u12ed \u1295\u1309\u1235 \u1270\u1270\u12ab\u1362 \u1265\u12d9 \u130a\u12dc \u1260\u1348\u1228\u1235 \u12e8\u121a\u130b\u120d\u1260\u12cd \u1218\u1276 \u12f0\u1228\u1303 \u120b\u12ed \u1290\u12cd\u1363 \u127d\u120e\u1273\u12cd\u121d \u12e8\u1270\u12cb\u1323\u1208\u1275 \u1290\u12cd\u1363 \u1265\u12d9 \u130a\u12dc \u12a9\u1295\u130d\u1349\u1295 \u1235\u1208\u121a\u1208\u121b\u1218\u12f1 \u1240\u120b\u120d \u12a5\u1293 \u1324\u1293\u121b \u1290\u12cd\u1362", "\u8056\u610f": "\u1264\u1275\u1205\u1293 \u1264\u1275\u1205 \u1230\u120b\u121d \u12a5\u1295\u12f2\u1230\u134d\u1295 \u1338\u120d\u12ed\u1363 \u1200\u1265\u1275 \u1348\u120d\u130b\u1363 \u1320\u1295\u12ad\u122e \u1218\u1225\u122b\u1275\u1363 \u1218\u1290\u1308\u12f5\u1363 \u1240\u12f5\u1218\u1205 \u12a5\u1295\u12f5\u1275\u1348\u122d\u1235\u1363 \u1275\u12f3\u122d \u12e8\u1270\u1233\u12ab \u12ed\u1206\u1293\u120d\u1363 \u120a\u12e9\u1302\u12eb \u1230\u120b\u121d \u12ed\u1230\u1323\u1273\u120d\u1363 \u1230\u12ce\u127d \u1260\u121c\u12f3 \u120b\u12ed \u12ed\u1206\u1293\u1209\u1363 \u12e8\u1210\u122d \u1275\u120e\u127d \u12ed\u1260\u1235\u120b\u1209\u1363 \u1235\u12f5\u1235\u1275 \u12a8\u1265\u1276\u127d \u12e8\u1260\u1230\u1209 \u12ed\u1206\u1293\u1209\u1362 , \u1230\u12cd \u121b\u130d\u1298\u1275 \u12a0\u1235\u1278\u130b\u122a \u12ed\u1206\u1293\u120d, \u12e8\u1205\u12dd\u1265 \u12ad\u1235 \u12a0\u1235\u1278\u12b3\u12ed \u12ed\u1206\u1293\u120d, \u1235\u12f0\u1275 \u12ed\u1320\u134b\u120d, \u12a5\u1293 \u1290\u1308\u122e\u127d \u1229\u1245 \u12ed\u1206\u1293\u1209, \u1260\u1270\u122b\u122e\u127d \u12a5\u1293 \u1260\u1218\u1243\u1265\u122d \u120b\u12ed \u1260\u123d\u1273\u12ce\u127d \u12ed\u1246\u12eb\u1209.", "\u53e4\u4eba": "\u12ab\u12a6 \u1352 \u122b\u1231\u1295 \u1295\u1309\u1220 \u1290\u1308\u1225\u1275 \u12a0\u12f5\u122d\u130e \u12eb\u12cd\u1303\u120d\u1362", "\u7b2cX\u7c64": "\u12d8\u1320\u1293 \u12a0\u121d\u1235\u1270\u129b \u12a0\u1238\u1293\u134a \u120e\u1270\u122a", "\u5bae": "\u1201\u130e\u1295\u130d"}