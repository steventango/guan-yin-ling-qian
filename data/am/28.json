{"\u7c64\u8a69\u7248\u672c\u4e8c": "\u1260\u121d\u1235\u122b\u1245 \u1260\u1328\u1228\u1243 \u1265\u122d\u1200\u1295 \u12cd\u1235\u1325 \u12e8\u121a\u12eb\u121d\u122d \u127b\u1295\u1301\u12cb\u1295 \u12a0\u1208, \u12a5\u1293 \u12f0\u1218\u1293\u12ce\u127d \u1208\u1325\u1242\u1275 \u130a\u12dc \u12ed\u1238\u134d\u1291\u1273\u120d \u12a5\u1293 \u1270\u12f0\u1265\u1240\u12cb\u120d.\n\u12a0\u1295\u12f3\u1295\u12f4 \u12ad\u1265 \u120a\u1206\u1295 \u12ed\u127d\u120b\u120d \u12a0\u1295\u12f3\u1295\u12f4 \u130d\u1295 \u120a\u130e\u12f5\u120d \u12ed\u127d\u120b\u120d\u1362\u12a0\u12ed\u12f0\u1208\u121d \u12e8\u121a\u1209 \u1270\u122b \u12c8\u122c\u12ce\u127d \u1293\u1278\u12cd\u1362", "\u8a69\u610f": "\u1260\u12da\u1205 \u1204\u12ad\u1233\u130d\u122b\u121d \u12cd\u1235\u1325 \u12eb\u1208\u12cd \u1328\u1228\u1243 \u1260\u12f0\u1218\u1293 \u1270\u1238\u134d\u1297\u120d\u1362\n\u1201\u1209\u121d \u1290\u1308\u122d \u12a0\u1201\u1295\u121d \u12ae\u121b \u12cd\u1235\u1325 \u1290\u12cd\u1362", "\u89e3\u66f0": "\u1270\u1295\u1233\u134b\u134a \u12f0\u1218\u1293\u12ce\u127d \u1328\u1228\u1243\u1295 \u12ed\u1238\u134d\u1293\u1209. \u1218\u1320\u122b\u1320\u122d \u12a0\u12eb\u1235\u1348\u120d\u130d\u121d\u1362 \u12f0\u1218\u1293\u12cd \u12a5\u1235\u12aa\u1230\u1260\u1235\u1265 \u12f5\u1228\u1235 \u1260\u1218\u1320\u1260\u1245 \u120b\u12ed\u1362 \u12eb\u1294 \u1275\u1228\u12f3\u1208\u1205\u1362", "\u7c64\u8a69\u6545\u4e8b\u4e00": "\u25c7\u1263\u12a6 \u130e\u1295\u130d \u12a5\u1274\u130c\u1295 \u120a \u12ed\u1348\u120d\u130b\u120d\n\u1260\u12d8\u1295\u130d \u1225\u122d\u12c8 \u1218\u1295\u130d\u1225\u1275 \u12e0\u1295\u12de\u1295\u130d \u12d8\u1218\u1290 \u1218\u1295\u130d\u1225\u1275 \u12a5\u1274\u130c \u120a (\u12a5\u1274\u130c \u120a) \u12e8\u1210\u1228\u121d \u1308\u1228\u12f5 \u1290\u1260\u1228\u127d \u1260Song Zhenzong \u121e\u1308\u1235 \u1270\u1230\u1325\u1277\u1275 \u12c8\u12f0 \u12cb\u1292 \u12a8\u134d \u1265\u120b\u1208\u127d\u1363 \u1338\u1290\u1230\u127d\u121d\u1362\n\u1290\u1308\u122d \u130d\u1295 \u1260\u12da\u12eb\u1295 \u130a\u12dc \u1241\u1263\u1275 \u120a\u12e9 \u12f4\u134d (\u1260\u128b\u120b \u12a5\u1274\u130c \u120a\u12e9) \u12ad\u1349 \u1210\u1233\u1265 \u1290\u1260\u122b\u1275 \u12a5\u1293 \u12e8\u12a5\u1274\u130c \u120a \u120d\u1305\u1295 \u1246\u12f3 \u1260\u1270\u1238\u1348\u1290 \u12f5\u1218\u1275 \u1208\u1218\u1270\u12ab\u1275 \u1208\u1303\u1295\u12f0\u1228\u1263\u12cd \u1309\u12a6 \u1201\u12cb\u12ed \u1309\u1266 \u1230\u1325\u1273 \u120d\u1307\u1295 \u1290\u1325\u1243 \u1208\u1295\u1309\u1220 \u1290\u1308\u1225\u1271 \u12e8\u12c8\u120b\u1305 \u120d\u1307 \u1218\u1206\u1291\u1295 \u1290\u1308\u1228\u127d\u12cd\u1362 \u1236\u1295\u130d \u12e0\u1295\u12de\u1295\u130d \u1270\u1293\u12f0\u12f0 \u12a5\u1293 \u120a \u12a5\u1274\u130c\u1295 \u12c8\u12f0 \u1240\u12dd\u1243\u12db\u12cd \u1264\u1270 \u1218\u1295\u130d\u1235\u1275 \u120b\u12a8\u1362\n\u1241\u1263\u1275 \u12f0 \u1295\u130d\u1225\u1275 \u1208\u1218\u1206\u1295 \u12ab\u12f0\u1308\u127d \u1260\u128b\u120b\u1363 \u12a8\u1264\u1270 \u1218\u1295\u130d\u1225\u1275 \u12a0\u121d\u120d\u1326 \u12e8\u1290\u1260\u1228\u12cd\u1295 \u12a5\u1274\u130c \u120a \u1208\u1218\u130d\u12f0\u120d \u12a0\u1245\u12f3 \u1290\u1260\u122d\u1362\n\u12a8\u12a5\u1208\u1273\u1275 \u12a0\u1295\u12f5 \u1240\u1295 \u1260\u1218\u1295\u1308\u12f5 \u120b\u12ed \u12e8\u121d\u1275\u1296\u1228\u12cd \u120a \u1292\u12eb\u1295\u130d\u12eb\u1295\u130d \u12e8\u1295\u1309\u1220 \u1290\u1308\u1225\u1271\u1295 \u120d\u12d1\u12ad \u1263\u12a6 \u12e0\u1295\u130d \u12a0\u1308\u1298\u127d\u12cd\u1293 \u1260\u12a0\u12ab\u1263\u1262\u12cd \u1260\u1325\u1260\u1243 \u120b\u12ed \u12a5\u12eb\u1208\u127d \u1235\u1208\u1215\u12ed\u12c8\u1277 \u120d\u121d\u12f7\u1295 \u1290\u1308\u1228\u127d\u12cd \u12a5\u1293 \u12e8\u12c8\u1245\u1271 \u12e8\u1295\u1309\u1220 \u1290\u1308\u1225\u1275 \u1218\u12dd\u1219\u122d \u122c\u1295\u12de\u1295\u130d \u12c8\u120b\u1305 \u12a5\u1293\u1275 \u12a5\u1295\u12f0\u1206\u1290\u127d \u1290\u1308\u1228\u127d\u12cd\u1362 . \u1263\u12a6 \u12e0\u1295\u130d \u1309\u12f3\u12e9\u1295 \u12ab\u12c8\u1240 \u1260\u128b\u120b \u120a \u1292\u12eb\u1295\u130d\u12eb\u1295\u130d\u1295 \u12ed\u12de \u12c8\u12f0 \u1264\u1302\u1295\u130d \u1260\u1218\u1218\u1208\u1235 \u1208\u1236\u1295\u130d \u122c\u1295\u12de\u1295\u130d \u122a\u1356\u122d\u1275 \u1260\u121b\u12f5\u1228\u130d \u1309\u12f3\u12e9\u1295 \u12a0\u12c8\u1240 \u1260\u1218\u1328\u1228\u123b\u121d \u12a5\u1293\u1275\u1293 \u120d\u1305 \u1270\u130d\u1263\u1265\u1270\u12cb\u120d\u1362 \u12a5\u1274\u130c \u1323\u12ed\u1271 \u120a\u12e9 \u12a5\u1293 \u12e8\u1283\u120b\u134a\u12cd \u1303\u1295\u12f0\u1228\u1263 \u1309\u12a1 \u1201\u12cb\u12ed \u12a0\u1295\u1308\u1273\u1278\u12cd\u1295 \u12f0\u134d\u1270\u12cd \u1270\u1293\u12d8\u12d9\u1362", "\u8056\u610f": "\u1264\u1275 \u12cd\u1235\u1325 \u12a0\u120d\u1270\u1278\u1308\u122d\u12a9\u121d\u1363 \u1208\u1308\u1295\u12d8\u1265 \u1325\u1260\u1243\u1363 \u1208\u1308\u1295\u12d8\u1265 \u127d\u130d\u122d\u1363 \u1260\u1295\u130d\u12f5 \u1235\u122b \u1218\u120d\u12ab\u121d \u12a5\u12f5\u120d\u1363 \u1275\u12f3\u122d\u1363 \u1218\u1325\u134e \u12d5\u12f5\u120d\u1363 \u1260\u1218\u1235\u12ad \u120b\u12ed \u12eb\u1209 \u1230\u12ce\u127d\u1363 \u12e8\u1210\u122d \u1275\u120e\u127d\u1363 \u12e8\u121b\u12ed\u1218\u1279 \u12a5\u1295\u1235\u1233\u1275\u1363 \u1230\u12ce\u127d\u1295 \u12e8\u121b\u130d\u1298\u1275 \u127d\u130d\u122d\u1363 \u12ad\u1235\u1363 \u12aa\u1233\u122b\u1363 \u1235\u12f0\u1275\u1363 \u12c8\u130d \u12a0\u1325\u1263\u1242\u1290\u1275\u1363 \u12e8\u1320\u134b \u1295\u1265\u1228\u1275 \u12e8\u121d\u12d5\u122b\u1263\u12cd\u12eb\u1295 \u1260\u123d\u1273\u12ce\u127d \u1208\u121b\u1308\u1308\u121d \u12a0\u1235\u1278\u130b\u122a \u1293\u1278\u12cd, \u12e8\u1270\u122b\u122b \u1218\u1243\u1265\u122d \u1270\u1203\u12f5\u1236", "\u53e4\u4eba": "\u1263\u12a6 \u130e\u1295\u130d \u12a5\u1274\u130c\u1295 \u120a \u12ed\u1348\u120d\u130b\u120d", "\u7b2cX\u7c64": "\u12e8\u1203\u12eb \u1235\u121d\u1295\u1270\u129b\u12cd \u120e\u1270\u122a \u12a0\u1238\u1293\u134a", "\u5bae": "\u1240\u1275\u122d \u1264\u1270\u1218\u1295\u130d\u1235\u1275"}